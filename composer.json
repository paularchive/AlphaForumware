{
	"name": "alphacrew/forum",
	"description": "AlphaForumware is forum system based on the Laravel PHP Framework.",
	"keywords": ["forum", "laravel", "alpha", "the alpha crew"],
	"license": "MIT",
	"type": "application",
	"require": {
		"laravel/framework": "4.2.*",
		"golonka/bbcodeparser": "1.3",
		"nesbot/carbon": "~1.13",
		"cviebrock/eloquent-sluggable": "~2.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
